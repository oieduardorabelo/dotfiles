"*":
  "atom-alignment":
    alignBy: [
      "=>"
      ":="
      ":"
      "="
      "from"
    ]
  "atom-beautify":
    general:
      _analyticsUserId: "64a4cd53-31ef-4f18-b63c-a2fff1f0f9dc"
    jsx: {}
  "auto-update-packages": {}
  "autocomplete-modules": {}
  "autocomplete-plus": {}
  core:
    audioBeep: false
    autoHideMenuBar: true
    automaticallyUpdate: false
    disabledPackages: [
      "metrics"
      "exception-reporting"
      "language-javascript"
      "welcome"
      "spell-check"
      "open-on-github"
      "link"
      "language-toml"
      "language-python"
      "language-ruby-on-rails"
      "language-php"
      "language-perl"
      "language-objective-c"
      "language-mustache"
      "language-less"
      "language-java"
      "language-hyperlink"
      "language-go"
      "language-csharp"
      "language-clojure"
      "language-c"
      "bookmarks"
      "background-tips"
      "update-package-dependencies"
      "flow-ide"
    ]
    excludeVcsIgnoredPaths: false
    hideGitIgnoredFiles: false
    ignoredNames: [
      ".git/*"
      ".tmp/*"
      "aas/*"
      "_ci/*"
      "_site/*"
      "build/*"
      "demo/*"
      "dist/*"
      "docker/*"
      "gradle/*"
      "node_binaries/*"
      "node_modules/*"
      "node_shrinkwrap/*"
      "war_resources/*"
      "bower_components/*"
    ]
    projectHome: "~/Projects/"
    restorePreviousWindowsOnStart: "no"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    autoIndent: true
    fontFamily: "Fira Code"
    fontSize: 18
    invisibles: {}
    lineHeight: 2
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
  "git-history":
    showDiff: false
  "js-hyperclick":
    extensions: [
      ".js"
      ".json"
      ".node"
      ".jsx"
    ]
    usePendingPanes: true
  "language-babel": {}
  linter: {}
  "linter-sass-lint": {}
  "linter-ui-default":
    panelHeight: 69
    showPanel: true
  minimap:
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      selection: true
      selectionDecorationsZIndex: 0
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      isDisabledIfNotInPackageJson: true
      showInStatusBar: true
    useEslint: true
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "sync-settings": {}
  term3: {}
  "tree-view": {}
  welcome:
    showOnStartup: false
