"*":
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "metrics"
      "exception-reporting"
    ]
    hideGitIgnoredFiles: true
    ignoredNames: [
      "_ci/*"
      "_site/*"
      "aas/*"
      "build/*"
      "demo/*"
      "dist/*"
      "docker/*"
      "gradle/*"
      "node_binaries/*"
      "node_modules/*"
      "node_shrinkwrap/*"
      "war_resources/*"
    ]
    projectHome: "~/Projects/"
  editor:
    fontFamily: "Hack"
    fontSize: 18
    invisibles: {}
    lineHeight: 2
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
  linter: {}
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
